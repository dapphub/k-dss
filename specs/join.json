  "stores" : {
    "#Adapter.vat" : "Vat",
    "#Adapter.ilk" : "Ilk",
    "#Adapter.gem" : "Gem"
  },
  "methods" : {
    "vat" : {
      "vars" : {
      },
      "stores" : {
      },
