    "join" : {
      "vars" : {
        "ABI_wad" : "uint256",
        "Wad" : "int256"
      },
      "stores" : {
      },
      "foreign_stores" : {
        "Vat" : {
          "#Vat.urns(Ilk, CALLER_ID, \"gem\")" : "#unsigned(Wad) => #unsigned(Wad +Int ABI_wad)"
        },
        "Gem" : {
          "#Gem.balances(CALLER_ID)" : "Bal_guy => Bal_guy -Int ABI_wad",
          "#Gem.balances(ACCT_ID)" : "Bal_adapter => Bal_adapter +Int ABI_wad"
        }
      },
      "requires" : [
        "#rangeSInt(256, ABI_wad)",
        "ABI_wad >=Int 0",
        "#rangeSInt(256, Wad +Int ABI_wad)",
        "#rangeUInt(256, Bal_guy -Int ABI_wad)",
        "#rangeUInt(256, Bal_adapter +Int ABI_wad)"
      ],
      "returns" : ".WordStack"
    },
    "exit" : {
      "vars" : {
        "ABI_wad" : "uint256",
        "Wad" : "int256"
      },
      "stores" : {
      },
      "foreign_stores" : {
        "Vat" : {
          "#Vat.urns(Ilk, CALLER_ID, \"gem\")" : "#unsigned(Wad) => #unsigned(Wad -Int ABI_wad)"
        },
        "Gem" : {
          "#Gem.balances(CALLER_ID)" : "Bal_guy => Bal_guy +Int ABI_wad",
          "#Gem.balances(ACCT_ID)" : "Bal_adapter => Bal_adapter +Int ABI_wad" 
        }
      },
      "requires" : [
        "#rangeSInt(256, ABI_wad)",
        "ABI_wad >=Int 0",
        "#rangeSInt(256, Wad -Int ABI_wad)",
        "Wad -Int ABI_wad >= 0",
        "#rangeUInt(256, Bal_guy +Int ABI_wad)",
        "#rangeUInt(256, Bal_adapter -Int ABI_wad)"
      ],
      "returns" : ".WordStack"
    }
  }
