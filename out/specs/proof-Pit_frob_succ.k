requires "../rules.k"

module PROOF-PIT_FROB_SUCC
  imports ETHEREUM-SIMULATION
  imports EVM
  imports RULES

// Pit_frob
rule
  <k> #execute ~> CONTINUATION => #halt ~> CONTINUATION </k>
  <exit-code> 1 </exit-code>
  <mode> NORMAL </mode>
  <schedule> BYZANTIUM </schedule>
  <analysis> .Map </analysis>
  <ethereum>
    <evm>
      <output> .WordStack </output>
      <statusCode> _ => EVMC_SUCCESS </statusCode>
      <callStack> VCallStack </callStack>
      <interimStates> _ </interimStates>
      <touchedAccounts> _ => _ </touchedAccounts>
      <callState>
        <program> #asMapOpCodes(#dasmOpCodes(#parseByteStack(""), BYZANTIUM)) </program>
        <programBytes> #parseByteStack("") </programBytes>
        <id> ACCT_ID </id>
        <caller> CALLER_ID </caller>
        <callData> #abiCallData("frob", #bytes32(ABI_ilk), #int256(ABI_dink), #int256(ABI_dart)) </callData>
        <callValue> 0 </callValue>
        <wordStack> .WordStack => _ </wordStack>
        <localMem> .Map => _ </localMem>
        <pc> 0 => _ </pc>
        <gas> VGas => _ </gas>
        <memoryUsed> 0 => _ </memoryUsed>
        <previousGas> _ => _ </previousGas>
        <static> false </static>
        <callDepth> VCallDepth => _ </callDepth>
      </callState>
      <substate>
        <selfDestruct> VSelfDestruct </selfDestruct>
        <log> _ => VLog </log>
        <refund> _ => VRefund </refund>
      </substate>
      <gasPrice> _ </gasPrice>
      <origin> ORIGIN_ID </origin>
      <previousHash> _ </previousHash>
      <ommersHash> _ </ommersHash>
      <coinbase> _ </coinbase>
      <stateRoot> _ </stateRoot>
      <transactionsRoot> _ </transactionsRoot>
      <receiptsRoot> _ </receiptsRoot>
      <logsBloom> _ </logsBloom>
      <difficulty> _ </difficulty>
      <number> _ </number>
      <gasLimit> _ </gasLimit>
      <gasUsed> _ </gasUsed>
      <timestamp> TIME </timestamp>
      <extraData> _ </extraData>
      <mixHash> _ </mixHash>
      <blockNonce> _ </blockNonce>
      <ommerBlockHeaders> _ </ommerBlockHeaders>
      <blockhash> _ </blockhash>
    </evm>
    <network>
      <activeAccounts> SetItem(ACCT_ID)
      SetItem(Vat) _ </activeAccounts>
      <accounts>
        <account>
          <acctID> Vat </acctID>
          <balance> Vat_balance </balance>
          <code> #parseByteStack("") </code>
          <storage> #Vat.wards[ACCT_ID] |-> (Can)
                      #Vat.ilks[ABI_ilk].rate |-> (Rate)
                      #Vat.ilks[ABI_ilk].take |-> (Take)
                      #Vat.ilks[ABI_ilk].Ink |-> (Ink_i => Ink_i +Int ABI_dink)
                      #Vat.ilks[ABI_ilk].Art |-> (Art_i => Art_i +Int ABI_dart)
                      #Vat.urns[ABI_ilk][CALLER_ID].ink |-> (Ink_iu => Ink_iu +Int ABI_dink)
                      #Vat.urns[ABI_ilk][CALLER_ID].art |-> (Art_iu => Art_iu +Int ABI_dart)
                      #Vat.gem[ABI_ilk][CALLER_ID] |-> (Gem_u => Gem_u -Int Take *Int ABI_dink)
                      #Vat.dai[CALLER_ID] |-> (Dai => Dai +Int Rate *Int ABI_dart)
                      #Vat.debt |-> (Debt => Debt +Int Rate *Int ABI_dart)
                      _:Map </storage>
          <nonce> _ </nonce>
        </account>
        <account>
          <acctID> ACCT_ID </acctID>
          <balance> BAL </balance>
          <code> #parseByteStack("") </code>
          <storage> #Pit.live |-> (Live)
                          #Pit.Line |-> (Line)
                          #Pit.vat |-> (Vat)
                          #Pit.ilks[ABI_ilk].line |-> (Line_i)
                          #Pit.ilks[ABI_ilk].spot |-> (Spot)
                          _:Map
           </storage>
          <nonce> _ </nonce>
        </account>
       ... 
      </accounts>
      <txOrder> _ </txOrder>
      <txPending> _ </txPending>
      <messages> _ </messages>
    </network>
  </ethereum>
requires #rangeAddress(ACCT_ID)
andBool #notPrecompileAddress(ACCT_ID)
andBool #rangeAddress(CALLER_ID)
andBool #rangeAddress(ORIGIN_ID)
andBool #rangeUInt(48, TIME)
andBool #rangeUInt(256, BAL)
andBool VCallDepth <=Int 1024
  andBool #rangeBytes(32, ABI_ilk)
  andBool #rangeSInt(256, ABI_dink)
  andBool #rangeSInt(256, ABI_dart)
  andBool #rangeUInt(256, Can)
  andBool #rangeUInt(256, Live)
  andBool #rangeUInt(256, Line)
  andBool #rangeAddress(Vat)
  andBool #rangeUInt(256, Spot)
  andBool #rangeUInt(256, Line_i)
  andBool #rangeUInt(256, Ink_i)
  andBool #rangeUInt(256, Art_i)
  andBool #rangeUInt(256, Ink_iu)
  andBool #rangeUInt(256, Art_iu)
  andBool #rangeUInt(256, Take)
  andBool #rangeUInt(256, Rate)
  andBool #rangeUInt(256, Gem_u)
  andBool #rangeUInt(256, Dai)
  andBool #rangeUInt(256, Debt)
  andBool #rangeUInt(256, Vat_balance)
  andBool #notPrecompileAddress(Vat)
  andBool ACCT_ID =/=Int Vat
  andBool VGas >Int 300000
  andBool Can ==Int 1
  andBool Rate =/=Int 0
  andBool (((((Art_iu +Int ABI_dart) *Int Rate) <=Int (#Ray *Int Spot)) andBool (((Debt +Int (Rate *Int ABI_dart))) <Int (#Ray *Int Line))) orBool (ABI_dart <=Int 0))
  andBool (((ABI_dart <=Int 0) andBool (ABI_dink >=Int 0)) orBool (((Ink_iu +Int ABI_dink) *Int Spot) >=Int ((Art_iu +Int ABI_dart) *Int Rate)))
  andBool Live ==Int 1
  andBool VCallDepth <Int 1024
  andBool #rangeUInt(256, Ink_i +Int ABI_dink)
  andBool #rangeUInt(256, Art_i +Int ABI_dart)
  andBool #rangeUInt(256, Ink_iu +Int ABI_dink)
  andBool #rangeUInt(256, Art_iu +Int ABI_dart)
  andBool #rangeUInt(256, Gem_u -Int Take *Int ABI_dink)
  andBool #rangeUInt(256, Dai +Int Rate *Int ABI_dart)
  andBool #rangeUInt(256, Debt +Int Rate *Int ABI_dart)
  andBool #rangeUInt(256, (Art_iu +Int ABI_dart) *Int Rate)
  andBool #rangeUInt(256, (Ink_iu +Int ABI_dink) *Int Spot)
  andBool #rangeUInt(256, #Ray *Int Spot)
  andBool #rangeUInt(256, #Ray *Int Line)
  andBool #rangeSInt(256, Take)
  andBool #rangeSInt(256, Take *Int ABI_dink)
  andBool #rangeSInt(256, Rate)
  andBool #rangeSInt(256, Rate *Int ABI_dart)

endmodule
