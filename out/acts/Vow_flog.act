behaviour flog of Vow
interface flog(uint48 t)

types

    Wait  : uint256
    Sin_t : uint256
    Sin   : uint256
    Woe   : uint256

storage

    #Vow.wait   |-> Wait
    #Vow.Sin    |-> Sin   => Sin - Sin_t
    #Vow.Woe    |-> Woe   => Woe + Sin_t
    #Vow.sin[t] |-> Sin_t => 0

iff

    t + Wait <= TIME

iff in range uint256

    t + Wait
    Sin - Sin_t
    Woe + Sin_t

if

    VGas > 300000
